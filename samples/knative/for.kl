{
  {for {let i 0} {lt % i 8} {set i {+ % i 1}}
    {writeln % "i : " i}

    {for {let j 0} {lt % j 4} {set j {+ % j 1}}
      {if {gt % j 2}
        {break}
      }
      {writeln % "    j : " j}
    }
    `` should print #ukn;
    {writeln % "  j in for i env " j}
    {if {gt % i 4}
      {break}
    }
    
  }
  `` should print #ukn;
  {writeln % "i in root env " i}
}